{
  "name": "starter-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate starter project for different backend setups.",
  "main": "index.js",
  "scripts": {
    "start": "npx nodemon --exec babel-node index.js",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.+(js|md|ts|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ],
    "testURL": "http://localhost/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged",
      "pre-push": "npm run test && lint-staged"
    }
  },
  "author": "Fred Hawk",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.1"
  },
  "dependencies": {
    "apollo-server": "^2.1.0",
    "dotenv": "^6.1.0",
    "faker": "^4.1.0",
    "graphql": "^0.13.2"
  }
}
